{
  "name": "server_beth",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production nodemon --exec babel-node ./src/bin/www",
    "local": "cross-env NODE_ENV=local-pc nodemon --exec babel-node ./src/bin/www",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/bin/www",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "nodemon",
    "heroku-prebuild": "export NPM_CONFIG_PRODUCTION=false; export NODE_ENV=; NPM_CONFIG_PRODUCTION=false NODE_ENV=development npm install --only=dev --dev",
    "heroku-postbuild": "export NPM_CONFIG_PRODUCTION=true; export NODE_ENV=production;"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "binance": "^2.9.4",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-bignumber": "3.0.0",
    "config": "^3.3.10",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "dotenv-flow": "^4.1.0",
    "express": "~4.16.1",
    "mongoose": "^5.9.20",
    "moralis": "^2.26.9",
    "morgan": "~1.9.1",
    "node-cron": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.2.3",
    "sequelize-utility": "^1.0.13",
    "socket.io": "^4.7.4",
    "solc": "^0.8.26",
    "truffle": "^5.11.5",
    "web3": "^4.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/node": "^7.24.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.24.7",
    "@babel/register": "^7.24.6",
    "babel-eslint": "^10.1.0",
    "cross-env": "^6.0.3",
    "nodemon": "^1.19.4"
  }
}
